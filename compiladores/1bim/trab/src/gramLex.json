{
   "comment": "JSON C Parser",
   "lex": {
      "rules": [
         ["\\s+"                              ,"/* skip whitespace */"],
         ["int"                               ,"return 'INT'"],
         ["double"                            ,"return 'DOUBLE'"],
         ["float"                             ,"return 'FLOAT'"],
         ["char"                              ,"return 'CHAR'"],
         ["\\("                                 ,"return 'LEFT_PARENTHESES'"],
         ["\\)"                                 ,"return 'RIGHT_PARENTHESES'"],
         [";"                                 ,"return 'SEMICOLON'"],
         [":"                                 ,"return 'COLON'"],
         // ["."                                 ,"return 'DOT'"],
         [","                                 ,"return 'COMMA'"],
         // ["'"                                 ,"return 'QUOTE'"],
         ["\""                                ,"return 'DQUOTE'"],
         ["\\["                                 ,"return 'LEFT_BRACKET'"],
         ["\\]"                                 ,"return 'RIGHT_BRACKET'"],
         ["\\{"                                 ,"return 'LEFT_COTTAR'"],
         ["\\}"                                 ,"return 'RIGHT_COTTAR'"],
         ["<="                                ,"return 'LESS_EQUAL'"],
         [">="                                ,"return 'GREATER_EQUAL'"],
         ["=="                                ,"return 'EQUAL'"],
         ["!="                                ,"return 'NOT_EQUAL'"],
         ["\\+="                                ,"return 'ADD_EQUAL'"],
         ["\\-="                                ,"return 'SUB_EQUAL'"],
         ["\\*="                                ,"return 'MULT_EQUAL'"],
         ["/="                                ,"return 'DIV_EQUAL'"],
         ["<"                                 ,"return 'LESS'"],
         [">"                                 ,"return 'GREATER'"],
         ["="                                 ,"return 'ATTR'"],
         ["\\*"                                 ,"return 'MULT'"],
         ["\\+"                                 ,"return 'ADD'"],
         ["\\-"                                 ,"return 'SUB'"],
         ["/"                                 ,"return 'DIV'"],
         // ["||"                                ,"console.log('Token OR'); return 'OR'"],
         ["!"                                 ,"return 'NOT'"],
         ["&&"                                ,"return 'AND'"],
         ["if"                                ,"return 'IF'"],
         ["while"                             ,"return 'WHILE'"],
         ["switch"                            ,"return 'SWITCH'"],
         ["case"                              ,"return 'CASE'"],
         ["break"                             ,"return 'BREAK'"],
         ["default"                           ,"return 'DEFAULT'"],
         ["else"                              ,"return 'ELSE'"],
         ["do while"                          ,"return 'DOWHILE'"],
         ["#"                                 ,"return 'HASHTAG'"],
         ["define"                            ,"return 'DEFINE'"],
         ["include"                            ,"return 'INCLUDE'"],
         ["void"                            ,"return 'VOID'"],
         ["main"                            ,"return 'MAIN'"],
         ["\\'.\\'"                              ,"return 'CHAR_LITERAL'"],
         ["[a-zA-Z][a-zA-Z0-9_]*"               ,"return 'IDENTIFIER'"],
         ["[0-9]*\\.[0-9]+([eE][+-][0-9]+)?"     ,"return 'FLOAT_LITERAL'"],
         ["[0-9]+"                              ,"return 'INT_LITERAL'"],
         ["."                                   ,"console.log('Erro léxico: caractere [', yytext, '] não reconhecido.')"],
         ["$"                             ,"return 'EOF'"]
      ]
   },

   "operators": [
      ["left", "+", "-"],
      ["left", "*", "/"],
      ["left", "^"],
      ["right", "!"],
      ["right", "%"],
      ["left", "UMINUS"]
   ],

   "bnf": {
      "expressions": [
         ["declaracaoVariavel EOF",   "return $1"]
      ],
      "palavraReservada": [
         ["tipo",  "return $1;"],
         ["IF",  "return $1;"],
         ["WHILE",  "return $1;"],
         ["SWITCH",  "return $1;"],
         ["CASE",  "return $1;"],
         ["BREAK",  "return $1;"],
         ["DEFAULT",  "return $1;"],
         ["ELSE",  "return $1;"],
         ["DOWHILE",  "return $1;"],
         ["HASHTAG",  "return $1;"],
         ["DEFINE",  "return $1;"],
         ["INCLUDE",  "return $1;"],
         ["VOID",  "return $1;"],
         ["MAIN", "return $1;"]
      ],
      "tipo": [
         ["INT",  ""],
         ["DOUBLE",  ""],
         ["FLOAT",  ""],
         ["CHAR",  ""]
      ],
      "valorLiteral": [
         ["FLOAT_LITERAL",  ""],
         ["INT_LITERAL",  ""],
         ["CHAR_LITERAL",  ""]
      ],
      "listaIdentificador": [
         ["IDENTIFIER", ""],
         ["IDENTIFIER COMMA listaIdentificador", ""]
      ],
      "listaIdentificadorValor": [
         ["IDENTIFIER ATTR valor", ""],
         ["IDENTIFIER ATTR valor COMMA listaIdentificadorValor", ""]
      ],
      "declaracaoVariavel": [
         ["tipo listaIdentificadorValor SEMICOLON", "console.log('Declaração com valor!')"],
         ["tipo listaIdentificador SEMICOLON", "console.log('Declaração sem valor!')"]
      ],
      "valor": [
         ["valorLiteral", ""],
         ["chamadaFuncao", ""]
      ],
      "chamadaFuncao": [
         ["IDENTIFIER LEFT_PARENTHESES RIGHT_PARENTHESES", ""],
         ["IDENTIFIER LEFT_PARENTHESES listaIdentificador RIGHT_PARENTHESES", ""]
      ],
      "comando": [
         ["DECLARACAO_VARIAVEL_VOID", "console.log('chegou auqi'); return $1;"]
      ],
      "token" :[
         ["LEFT_PARENTHESES",  "return $1;"],
         ["RIGHT_PARENTHESES",  "return $1;"],
         ["SEMICOLON",  "return $1;"],
         ["COLON",  "return $1;"],
         ["DOT",  "return $1;"],
         ["COMMA",  "return $1;"],
         ["QUOTE",  "return $1;"],
         ["DQUOTE",  "return $1;"],
         ["LEFT_BRACKET",  "return $1;"],
         ["RIGHT_BRACKET",  "return $1;"],
         ["LEFT_COTTAR",  "return $1;"],
         ["RIGHT_COTTAR",  "return $1;"],
         ["LESS_EQUAL",  "return $1;"],
         ["GREATER_EQUAL",  "return $1;"],
         ["EQUAL",  "return $1;"],
         ["NOT_EQUAL",  "return $1;"],
         ["ADD_EQUAL",  "return $1;"],
         ["SUB_EQUAL",  "return $1;"],
         ["MULT_EQUAL",  "return $1;"],
         ["DIV_EQUAL",  "return $1;"],
         ["LESS",  "return $1;"],
         ["GREATER",  "return $1;"],
         ["ATTR",  "return $1;"],
         ["MULT",  "return $1;"],
         ["ADD",  "return $1;"],
         ["SUB",  "return $1;"],
         ["DIV",  "return $1;"],
         ["OR",  "return $1;"],
         ["NOT",  "return $1;"],
         ["AND",  "return $1;"],
         ["IDENTIFIER",  "return $1;"]
      ]
   }
}
